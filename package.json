{
  "name": "penang",
  "description": "Membership and Subscription Theme",
  "demo": "https://penang.aspirethemes.com/",
  "version": "1.3.2",
  "engines": {
    "ghost": ">=5.0.0"
  },
  "keywords": [
    "ghost-theme"
  ],
  "scripts": {
    "postinstall": "bower install"
  },
  "dependencies": {
    "@primer/css": "^15.2.0",
    "clipboard": "^2.0.8",
    "concat": "^3.0.0",
    "evil-icons": "^1.10.1",
    "fs": "0.0.2",
    "gulp-concat": "^2.6.0",
    "gulp-util": "^3.0.7",
    "headroom.js": "^0.10.4",
    "lazysizes": "^5.3.2",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.25.0",
    "sass-mq": "^5.0.1"
  },
  "devDependencies": {
    "ansi-colors": "^1.1.0",
    "beeper": "^1.1.1",
    "dart-sass": "^1.25.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-jshint": "^2.1.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-rtlcss": "^1.4.1",
    "gulp-sass": "^5.0.0",
    "gulp-size": "^4.0.1",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^4.2.0",
    "jshint": "^2.13.1",
    "jshint-stylish": "^2.2.1",
    "node-sass": "^6.0.1",
    "run-sequence": "^2.2.1",
    "sass": "^1.35.1"
  },
  "author": {
    "name": "Aspire Themes",
    "email": "aspirethemes@gmail.com",
    "url": "https://aspirethemes.com/"
  },
  "config": {
    "custom": {
      "enable_search": {
        "type": "boolean",
        "default": true
      },
      "enable_dark_mode": {
        "type": "boolean",
        "default": true
      },
      "dark_mode_accent_color": {
        "type": "color",
        "default": "#0071e3"
      },
      "dark_mode_publication_logo": {
        "type": "image"
      },
      "instagram_page": {
        "type": "text",
        "default": ""
      },
      "pinterest_page": {
        "type": "text",
        "default": ""
      },
      "linkedin_page": {
        "type": "text",
        "default": ""
      },
      "tag_slugs_for_home_sections": {
        "type": "text",
        "group": "homepage"
      },
      "enable_home_authors_section": {
        "type": "boolean",
        "default": true,
        "group": "homepage"
      }
    },
    "posts_per_page": 12,
    "card_assets": {
      "exclude": [
        "bookmark",
        "gallery",
        "blockquote"
      ]
    },
    "image_sizes": {
      "xxs": {
        "width": 48
      },
      "xs": {
        "width": 128
      },
      "sm": {
        "width": 176
      },
      "md": {
        "width": 320
      },
      "ml": {
        "width": 588
      },
      "lg": {
        "width": 636
      },
      "xl": {
        "width": 1272
      },
      "xxl": {
        "width": 2640
      }
    }
  }
}
